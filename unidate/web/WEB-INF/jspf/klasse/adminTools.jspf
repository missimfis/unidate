                                for(User aktuell:klasseOhneAdmin){      
                                   %><option value="<%out.print(aktuell.getUsername());%>"><%out.print(aktuell.getUsername());%></option><%
                } %>
                            </select>
                        </div>
                        <div class="small-9 columns">
                            <A HREF="administration"><input id="choosebutton" 
                                                                class="button tiny" type="submit" value="Als Admin bestimmen"/></A>
                        </div>
                    </form>
                </label>
            </div>